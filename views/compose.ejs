<%- include('partials/header') -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label class= "form-label" for="title">Title</label>
    <input class= "form-control" id="title" type="text" name="postTitle" value="">
  </div>

  <div class="form-group">
    <label class= "form-label" for="post">Post</label>
    <textarea class= "form-control" id= "post" name="postBody" rows="8" cols="30"></textarea>
  </div>

  <div class="">
    <button class="btn btn-primary" type="submit" name="">Publish</button>
  </div>

</form>

<%- include('partials/footer') -%>
